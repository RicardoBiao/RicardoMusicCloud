.content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 646rpx;
  height: 78rpx;
  background-color: #0e0b1f;
  margin-bottom: 20rpx;
}
.title-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.picture {
  display: -webkit-inline-box;
  display: -webkit-inline-flex;
  display: inline-flex;
  width: 64rpx;
  height: 64rpx;
  margin: auto 0;
}
.setting {
  position: absolute;
  right: 6vw;
  width: 50rpx;
  height: 50rpx;
  margin: auto 0;
}

