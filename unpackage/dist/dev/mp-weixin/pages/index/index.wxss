page {
  width: 100%;
  height: 100%;
  background-color: #0e0b1f;
}
.top-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  height: 15vw;
}
.top-box .title {
  display: -webkit-inline-box;
  display: -webkit-inline-flex;
  display: inline-flex;
  font-size: 13vw;
}
.top-box image {
  width: 7vw;
  height: 7vw;
  margin: auto 0;
  margin-right: 7vw;
}
text {
  display: block;
  height: 12vw;
  line-height: 12vw;
  font-family: Helvetica;
  font-size: 6vw;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0vw;
  color: #eeeeee;
  padding: 0 6vw;
}
.albums-title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
video {
  width: 654rpx;
  height: 376rpx;
  margin: 0 6vw;
}
.swiper-tranform {
  height: 53vw;
  background-color: #007AFF;
}
.video {
  height: 50vw;
  background-color: #007AFF;
}
.mini-music {
  height: 50vw;
  background-color: #007AFF;
}
.mini-music2 {
  height: 10vw;
  background-color: #ffba66;
}
.music-play {
  position: fixed;
  z-index: 100;
  bottom: 0;
  width: 100%;
  height: 19vw;
  background-color: #e149ff;
}
.music-play-bottom {
  width: 100%;
  height: 19vw;
}

