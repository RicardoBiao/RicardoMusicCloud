
.content {
	background-color: #0e0b1f;
	height: 100%;
}
.title {
	height: 15vw;
	margin: 0 0 8vw 6vw;
	font-family: Helvetica;
	font-size: 13vw;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0vw;
	color: #eeeeee;
}
.other-title {
	height: 13vw;
	margin-bottom: 5vw;
	background-color: #3F536E;
}
.popular {
	margin-top: 11vw;
	margin-left: 6vw;
	font-family: Helvetica;
	font-size: 6vw;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0vw;
	color: #eeeeee;
}
.music-box {
	width: 100vw;
	height: 21vw;
	margin: 5vw 4vw 5vw 6vw;
}
.music-image {
	width: 21vw;
